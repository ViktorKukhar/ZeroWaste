<turbo-frame id="diapers_categories">
  <table>
    <thead>
      <tr>
        <th>Period Start</th>
        <th>Period End</th>
        <th>Usage Amount</th>
        <th>Price</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @diapers_periods.each do |diapers_period| %>
        <tr>
          <td><%= diapers_period.period_start %></td>
          <td><%= diapers_period.period_end %></td>
          <td><%= diapers_period.usage_amount %></td>
          <td><%= diapers_period.price %></td>
          <td><%= link_to '[ Edit ]', edit_diapers_period_account_site_setting_path(id: diapers_period.id), data: { turbo_frame: 'diapers_categories', turbo_frame: 'diapers_categories' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to '[Add New Period]', new_account_site_setting_diapers_period_path(category_id: @category.id), class: "btn-grey" %>
  <%= link_to '[Back to Categories]', show_diapers_categories_account_site_setting_path, method: :get, data: { turbo_frame: 'diapers_categories' }, class: "btn-grey" %>

</turbo-frame>
